{
    "defaultRenderer": "fiori2",
    "bootstrapPlugins": {
        "RuntimeAuthoringPlugin": {
            "component": "sap.ushell.plugins.rta"
        },
        "PersonalizePlugin": {
            "component": "sap.ushell.plugins.rta-personalize"
        }
    },
    "services": {
        "NavTargetResolution": {
            "config": {
                "allowTestUrlComponentConfig": true,
                "enableClientSideTargetResolution": true
            }
        },
        "EndUserFeedback": {
            "adapter": {
                "config": {
                    "enabled": true
                }
            }
        }
    },
    "applications": {
        "selfie-ui": {
            "title": "Selfie with the Dev Advocates",
            "description": "Devtoberfest 2025 Selfie with the Dev Advocates",
            "additionalInformation": "SAPUI5.Component=sap.selfie",
            "applicationType": "URL",
            "url": "./selfie",
            "navigationMode": "embedded"
        },
        "profile-ui": {
            "title": "SAP Community Profile and Badge Signature Tool",
            "description": "SAP Community Profile and Badge Signature Tool",
            "additionalInformation": "SAPUI5.Component=sap.profile",
            "applicationType": "URL",
            "url": "./profile",
            "navigationMode": "embedded"
        },
        "tags-ui": {
            "title": "All SAP Managed Tags",
            "description": "Check out the comprehensive A-Z list of SAP managed tags, our curated vocabulary to help you categorize and enhance your content creation in SAP Community.",
            "additionalInformation": "SAPUI5.Component=sap.tags",
            "applicationType": "URL",
            "url": "./tags",
            "navigationMode": "embedded"
        }
    }
}